{
  "name": "gigagent",
  "version": "1.0.0",
  "scripts": {
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "install-server": "cd server && npm install",
    "start-server": "cd server && npm start",
    "deploy": "npm run install-client && npm run build-client && npm run install-server && npm run start-server"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongodb": "^6.13.0",
    "mongoose": "^8.9.6",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9"
  }
}
